				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="dst_dir">
				<xs:annotation>
					<xs:documentation><![CDATA[
分发输出目录。可定义环境变量，如${dst_dir}。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="rdst_dir" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
分发输出远程目录。可定义环境变量，如${rdst_dir}。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="pattern" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
回退时使用的正则表达式。可定义环境变量。默认为^.*\\.${SVC_KEY}$
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="options" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
INTERNAL：内部格式。
SAME_FILE：允许不同格式写入同一文件。
SPLIT：一条记录根据某个字段分成多条记录输出。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:list>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="INTERNAL"/>
								<xs:enumeration value="SAME_FILE"/>
								<xs:enumeration value="SPLIT"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:list>
				</xs:simpleType>
			</xs:element>
			<xs:element name="head" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
头记录格式定义。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="distribute_record"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="body" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
体记录格式定义。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="distribute_record"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="tail" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
尾记录格式定义。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="distribute_record"/>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="distribute_record">
		<xs:all>
			<xs:element name="delimiter" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
记录间分隔符对应的ASCII
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="options" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
RETURN：每行记录有回车。
FIXED：定长输出。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:list>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="RETURN"/>
								<xs:enumeration value="FIXED"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:list>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fields">
				<xs:annotation>
					<xs:documentation><![CDATA[
分发字段列表定义。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="field" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation><![CDATA[
分发字段定义。
								]]></xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:group ref="distribute_field"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="distribute_field">
		<xs:all>
			<xs:element name="from_global" default="N" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
字段是否来自全局变量。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Y"/>
						<xs:enumeration value="N"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="field_name">
				<xs:annotation>
					<xs:documentation><![CDATA[
来源的字段名称。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="field_type" default="STRING" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
INT：整型
LONG：长整型
FLOAT：浮点型
DOUBLE：双精度浮点型
STRING：字符串型
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="INT"/>
						<xs:enumeration value="LONG"/>
						<xs:enumeration value="FLOAT"/>
						<xs:enumeration value="DOUBLE"/>
						<xs:enumeration value="STRING"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="align_mode" default="INTERNAL" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
LEFT：左对齐方式
INTERNAL：内部对齐方式
RIGHT：右对齐方式
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="LEFT"/>
						<xs:enumeration value="INTERNAL"/>
						<xs:enumeration value="RIGHT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="field_size" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
字段长度。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fill" default=" " minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
填充符。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="precision" default="0" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
小数点后保留N位。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="action" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
空：无动作
ADD_INT：整型累加
ADD_LONG：长整型累加
ADD_FLOAT：浮点累加
ADD_DOUBLE：双精度浮点累加
INC_INT：整型自增
MIN_INT：整型取最小
MIN_LONG：长整型取最小
MIN_FLOAT：浮点取最小
MIN_DOUBLE：双精度浮点取最小
MIN_STR：字符串取最小
MAX_INT：整型取最大
MAX_LONG：长整型取最大
MAX_FLOAT：浮点取最大
MAX_DOUBLE：双精度浮点取最大
MAX_STR：字符串取最大
SPLIT_FIELD：拆单数组，例如3000, 1000,100….，会拆分为多条话单，此字段按“,”分隔输出多条记录。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ADD_INT"/>
						<xs:enumeration value="ADD_LONG"/>
						<xs:enumeration value="ADD_FLOAT"/>
						<xs:enumeration value="ADD_DOUBLE"/>
						<xs:enumeration value="INC_INT"/>
						<xs:enumeration value="MIN_INT"/>
						<xs:enumeration value="MIN_LONG"/>
						<xs:enumeration value="MIN_FLOAT"/>
						<xs:enumeration value="MIN_DOUBLE"/>
						<xs:enumeration value="MIN_STR"/>
						<xs:enumeration value="MAX_INT"/>
						<xs:enumeration value="MAX_LONG"/>
						<xs:enumeration value="MAX_FLOAT"/>
						<xs:enumeration value="MAX_DOUBLE"/>
						<xs:enumeration value="MAX_STR"/>
						<xs:enumeration value="SPLIT_FIELD"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="stat">
		<xs:all>
			<xs:element name="class_name">
				<xs:annotation>
					<xs:documentation><![CDATA[
统计入库处理类名。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="sa_output"/>
						<xs:enumeration value="sa_ostat"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="items">
				<xs:annotation>
					<xs:documentation><![CDATA[
分发项目定义列表。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="item" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation><![CDATA[
分发项目定义。
								]]></xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:group ref="stat_item"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="stat_item">
		<xs:all>
			<xs:element name="options">
				<xs:annotation>
					<xs:documentation><![CDATA[
统计入库选项：
INSERT：插入
UPDATE：更新
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:list>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="INSERT"/>
								<xs:enumeration value="UPDATE"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:list>
				</xs:simpleType>
			</xs:element>
			<xs:element name="rule_condition" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
匹配规则，返回0表示匹配上。可用的变量有：
1. 全局变量
2. $i - 输出变量
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rule_table_name">
				<xs:annotation>
					<xs:documentation><![CDATA[
分发输出的文件名规则，可用的变量有：
1. 全局变量
2. $i - 输出变量
3. #0 - 输出表名
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fields">
				<xs:annotation>
					<xs:documentation><![CDATA[
统计入库所有字段定义。
					]]></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="field" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation><![CDATA[
统计入库字段定义。
								]]></xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:group ref="stat_field"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="stat_field">
		<xs:all>
			<xs:element name="from_global" default="N" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
字段是否来自全局变量。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Y"/>
						<xs:enumeration value="N"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="field_name">
				<xs:annotation>
					<xs:documentation><![CDATA[
来源的字段名称。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="element_name" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
表中的字段名称，默认为来源字段名称。
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="operation" default="=" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
在条件语句中的比较操作符。
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="insert_desc" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
插入字段描述。
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="update_desc" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
更新字段描述。
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="is_key" default="N" minOccurs="0">
				<xs:annotation>
					<xs:documentation><![CDATA[
是否为关键字字段。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Y"/>
						<xs:enumeration value="N"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:group>
	<xs:group name="arg">
		<xs:all>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation><![CDATA[
参数名称。
					]]></xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="rule_dup"/>
						<xs:enumeration value="tables"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="value">
				<xs:annotation>
					<xs:documentation><![CDATA[
参数值。
					]]></xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:group>
</xs:schema>
